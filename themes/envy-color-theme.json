		"settings.checkboxBorder": "#191a21",
		"settings.dropdownBorder": "#191a21",
		"settings.numberInputBorder": "#191a21",
		"settings.textInputBorder": "#191a21",
		"sideBarSectionHeader.border": "#191a21",
